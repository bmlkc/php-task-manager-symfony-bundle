{
  "name": "sunvalley-technologies/php-task-manager-symfony-bundle",
  "description": "Symfony bundle for PHP Task Manager",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "SunValley Technologies",
      "email": "info@sunvalley.com.es"
    }
  ],
  "require": {
    "symfony/framework-bundle": "^3.0 || ^4.0",
    "sunvalley-technologies/php-task-manager": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0|~7.0",
    "symfony/console": "^3.0 || ^4.0",
    "symfony/phpunit-bridge": "^3.0 || ^4.0"
  },
  "autoload": {
    "psr-4": {
      "SunValley\\TaskManager\\Symfony\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SunValley\\TaskManager\\Symfony\\Tests\\": "/Tests/"
    }
  }
}
